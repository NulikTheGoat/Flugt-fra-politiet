{
  "name": "flugt-fra-politiet",
  "version": "1.1.0",
  "description": "A fast-paced 3D car chase game! Outrun the cops, collect coins, and upgrade your ride!",
  "main": "script_backup.js",
  "scripts": {
    "lint": "eslint . --ext .js,.cjs",
    "lint:fix": "eslint . --ext .js,.cjs --fix",
    "format": "prettier --write .eslintrc.cjs .prettierrc package.json package-lock.json server.js .github/workflows/*.yml",
    "format:check": "prettier --check .eslintrc.cjs .prettierrc package.json package-lock.json server.js .github/workflows/*.yml",
    "test": "npx playwright test",
    "test:fast": "npx playwright test --workers=4",
    "test:failed": "npx playwright test --last-failed",
    "test:only": "npx playwright test --grep",
    "test:quick": "npx playwright test tests/regression/quick.spec.js --workers=4",
    "test:speed": "npx playwright test speed-limit.spec.js",
    "test:values": "node tests/test-speed-values.cjs",
    "test:debug": "npx playwright test --debug --headed",
    "test:headed": "npx playwright test --headed --workers=1",
    "test:regression": "npx playwright test tests/regression/",
    "test:regression:fast": "npx playwright test tests/regression/ --workers=4",
    "test:core": "npx playwright test tests/regression/core-gameplay.spec.js",
    "test:ui": "npx playwright test tests/regression/ui-elements.spec.js",
    "test:physics": "npx playwright test tests/regression/physics-collision.spec.js",
    "test:shop": "npx playwright test tests/regression/shop-economy.spec.js",
    "test:police": "npx playwright test tests/regression/police-ai.spec.js",
    "test:mp": "npx playwright test tests/regression/multiplayer.spec.js",
    "test:world": "npx playwright test tests/regression/world-rendering.spec.js",
    "test:all": "npm run test:values && npx playwright test --workers=4",
    "test:ci": "npx playwright test --workers=2 --retries=2 --reporter=line",
    "typecheck": "tsc --noEmit --project typecheck.json",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NulikTheGoat/Flugt-fra-politiet.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/NulikTheGoat/Flugt-fra-politiet/issues"
  },
  "homepage": "https://github.com/NulikTheGoat/Flugt-fra-politiet#readme",
  "dependencies": {
    "cannon-es": "^0.20.0",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "three": "^0.160.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^22.10.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "playwright": "^1.58.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^7.3.1"
  }
}
